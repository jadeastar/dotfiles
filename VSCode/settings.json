  "workbench.iconTheme": "vsc-jetbrains-icons-enhanced",
  "remote.SSH.defaultExtensions": [
    "gitpod.gitpod-remote-ssh"
  ],
  "remote.SSH.configFile": "/tmp/gitpod_ssh_config-7921-ibDHm0UzMqst",
  "git.enableSmartCommit": true,
  "git.autofetch": true
